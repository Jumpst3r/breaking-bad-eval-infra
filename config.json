{
    "version":1.0,
    "toolchains": [
        {
            "x86-64-core-i7--glibc--stable-2018.11-1": {
                "url": "https://toolchains.bootlin.com/downloads/releases/toolchains/x86-64-core-i7/tarballs/x86-64-core-i7--glibc--stable-2018.11-1.tar.bz2",
                "rootfs": "x86_64-buildroot-linux-gnu/sysroot",
                "libdir": "lib64/",
                "prefix": "bin/x86_64-linux-",
                "compileOptions": [
                    {
                        "name": "openssl",
                        "buildcmd": "./Configure linux-x86_64 --cross-compile-prefix=$(pwd)/../toolchain/bin/x86_64-linux- $CFLAGS",
                        "cflags": ""
                    },
                    {
                        "name": "botan",
                        "buildcmd": "./configure.py --cc-bin=$(pwd)/../toolchain/bin/x86_64-linux-g++ --extra-cxxflags=\"$CFLAGS\" && cat Makefile | grep -v '^WARN_FLAGS' > Makefile2 && mv Makefile2 Makefile",
                        "cflags": ""
                    },
                    {
                        "name": "wolfssl",
                        "buildcmd": "./autogen.sh && ./configure  --enable-aesni --enable-intelasm --enable-opensslextra --enable-all-crypto CC=$(pwd)/../toolchain/bin/x86_64-linux-gcc AR=$(pwd)/../toolchain/bin/x86_64-linux-ar LD=$(pwd)/../toolchain/bin/x86_64-linux-ld CFLAGS=\"$CFLAGS\" --prefix=$(pwd)/../toolchain/x86_64-buildroot-linux-gnu/sysroot/",
                        "cflags": ""
                    },
                    {
                        "name": "mbedtls",
                        "buildcmd": "SHARED=1 CC=$(pwd)/../toolchain/bin/x86_64-linux-gcc AR=$(pwd)/../toolchain/bin/x86_64-linux-ar LD=$(pwd)/../toolchain/bin/x86_64-linux-ld CFLAGS=\"$CFLAGS\" make lib",
                        "cflags": ""
                    }
                ]
            },
